{
  "manifest_version": 3,
  "name": "Flashcard New Tab",
  "version": "1.0",
  "description": "Show flashcards every time you open a new tab",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Manage Flashcards"
  }
}
